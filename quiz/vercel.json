{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "database.json", 
                    "api.js",
                    "elements.js",
                    "questions.js",
                    "quiz.js",
                    "results.js",
                    "main.js"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}